  },
  "devDependencies": {
    "all-contributors-cli": "^5.2.1",
    "ava": "^0.25.0",
    "co": "^4.6.0",
    "eslint": "^5.7.0",
    "eslint-config-precure": "^3.2.0",
    "husky": "^1.1.2",
    "is-plain-obj": "^1.1.0",
    "lint-staged": "^7.3.0",
    "lodash.isequal": "^4.4.0",
