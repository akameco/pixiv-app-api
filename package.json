{
  "name": "pixiv-app-api",
  "version": "0.15.0",
  "description": "Promise base pixiv API client",
  "license": "MIT",
  "repository": "akameco/pixiv-app-api",
  "author": {
    "name": "akameco",
    "email": "akameco.t@gmail.com",
    "url": "https://akameco.github.io"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "add-contributor": "all-contributors add",
    "fmt": "prettier --write '**/*.{ts,js,json,md}'",
    "build": "tsc",
    "lint": "eslint --fix src/*.ts",
    "test": "jest",
    "prepublish": "yarn build",
    "start": "tsc && node dist/temp.js"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "pixiv",
    "api-client",
    "pixiv-app",
    "pixiv-app-api",
    "pixiv-api"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "camelcase-keys": "^6.0.1",
    "decamelize-keys": "^1.1.0"
  },
  "devDependencies": {
    "@akameco/tsconfig": "^0.3.0",
    "@types/camelcase-keys": "^5.1.1",
    "@types/is-plain-obj": "^2.0.2",
    "@types/jest": "^24.0.18",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^12.7.4",
    "all-contributors-cli": "^6.9.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-precure": "^5.1.0",
    "husky": "^3.0.5",
    "is-plain-obj": "^2.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "lodash.isequal": "^4.4.0",
    "pixiv-img": "^1.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
